{
    "devCommand": "uvicorn src:app --host 0.0.0.0 --port 8000",
    "builds": [
        {
            "src": "src/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "35 0 * * *"
        }
    ]
}